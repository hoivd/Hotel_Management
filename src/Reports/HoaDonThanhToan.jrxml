	<queryString>
		<![CDATA[SELECT
    pdp.madatphong,
    tt.manv AS nvtt,
    nv_ten_nvtt.tennv AS tennvtt,
    kh.makh,
    kh.tenkh,
    pdp.tienphong,
    pdp.phuphi,
    tt.mathanhtoan,
    tt.thanhtien,
    tt.ngaylap,
    tt.tienkhachdua,
    tt.hinhthucthanhtoan,
    pdp.tientratruoc,
    pdp.manv AS nvdp,
    nv_ten_nvdp.tennv AS tennvdp
FROM
    phieudatphong pdp
INNER JOIN thanhtoan tt ON pdp.madatphong = tt.madatphong
INNER JOIN khachhang kh ON pdp.makh = kh.makh
INNER JOIN nhanvien nv_ten_nvtt ON tt.manv = nv_ten_nvtt.manv
INNER JOIN nhanvien nv_ten_nvdp ON pdp.manv = nv_ten_nvdp.manv
WHERE
    tt.mathanhtoan = (
        SELECT MAX(mathanhtoan)
        FROM thanhtoan
    )]]>
	</queryString>
